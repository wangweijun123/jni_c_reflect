<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="c反射回调java方法"
        android:onClick="cCalljavaByReflect"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="调用传递int类型参数的方法"
        android:onClick="callbackint"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="调用传递string类型参数的方法"
        android:onClick="callbackString"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="调用传递string类型参数弹出土司的方法"
        android:onClick="callbackToast"/>

</LinearLayout>